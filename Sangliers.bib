@article{agarwalBoneMorphologiesHistories2016,
  title = {Bone Morphologies and Histories: {{Life}} Course Approaches in Bioarchaeology},
  shorttitle = {Bone Morphologies and Histories},
  author = {Agarwal, Sabrina C.},
  date = {2016},
  journaltitle = {American Journal of Physical Anthropology},
  volume = {159},
  number = {S61},
  pages = {130--149},
  issn = {1096-8644},
  doi = {10.1002/ajpa.22905},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ajpa.22905},
  urldate = {2024-01-26},
  abstract = {The duality of the skeleton as both a biological and cultural entity has formed the theoretical basis of bioarchaeology. In recent years bioarchaeological studies have stretched the early biocultural concept with the adoption of life course approaches in their study design and analyses, making a significant contribution to how we think about the role of postnatal plasticity. Life course theory is a conceptual framework used in several scientific fields of biology and the social sciences. Studies that emphasize life course approaches in the examination of bone morphology in the past are united in their interrogation of human life as a result of interrelated and cumulative events over not only the timeframe of individuals, but also over generations at the community level. This article provides an overview of the theoretical constructs that utilize the life course concept, and a discussion of the different ways these theories have been applied to thinking about trajectories of bone morphology in the past, specifically highlighting key recent studies that have used life course approaches to understand the influence of growth, stress, diet, activity, and aging on the skeleton. The goal of this article is to demonstrate the scope of contemporary bioarchaeological studies that illuminate the importance of environmental and behavioral influence on bone morphology. Understanding how trajectories of bone growth and morphology can be altered and shaped over the life course is critical not only for bioarchaeologists, but also researchers studying bone morphology in living nonhuman primates and fossil primate skeletons. Am J Phys Anthropol 159:S130–S149, 2016. © 2016 American Association of Physical Anthropologists},
  langid = {english},
  keywords = {bone development,bone morphology,DOHaD,fetal programming,plasticity},
  file = {C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\4FK5C6MV\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\DEFFY7B6\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\E67F8USJ\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\HVC6F3ST\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\P4P5FR8K\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\XBBJWU33\\Agarwal - 2016 - Bone morphologies and histories Life course appro.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\3FJV7FTY\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\5T62WCQP\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\63IZ479N\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\BESEHG4W\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\EVSIUFJL\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\KQUXELRD\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\N5CJ9CUA\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\QIJVQUAY\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\QMHT4MWQ\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\T3VWAZP8\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\TWJ5FRAZ\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\UN8JJS7A\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\V326LUNA\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\V49S9332\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\VZHK7R38\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\W8V5WYZF\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\WQCSSERG\\ajpa.html;C\:\\Users\\Bordeaux Montaigne\\Zoter