@article{doubeTrabecularBoneScales2011b,
  title = {Trabecular Bone Scales Allometrically in Mammals and Birds},
  author = {Doube, Michael and K?osowski, Micha? M. and Wiktorowicz-Conroy, Alexis M. and Hutchinson, John R. and Shefelbine, Sandra J.},
  date = {2011-03-09},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {278},
  number = {1721},
  pages = {3067--3073},
  publisher = {Royal Society},
  doi = {10.1098/rspb.2011.0069},
  url = {https://royalsocietypublishing.org/doi/full/10.1098/rspb.2011.0069},
  urldate = {2024-04-19},
  abstract = {Many bones are supported internally by a latticework of trabeculae. Scaling of whole bone length and diameter has been extensively investigated, but scaling of the trabecular network is not well characterized. We analysed trabecular geometry in the femora of 90 terrestrial mammalian and avian species with body masses ranging from 3 g to 3400 kg. We found that bone volume fraction does not scale substantially with animal size, while trabeculae in larger animals' femora are thicker, further apart and fewer per unit volume than in smaller animals. Finite element modelling indicates that trabecular scaling does not alter the bulk stiffness of trabecular bone, but does alter strain within trabeculae under equal applied loads. Allometry of bone's trabecular tissue may contribute to the skeleton's ability to withstand load, without incurring the physiological or mechanical costs of increasing bone mass.},
  issue = {1721},
  keywords = {allometry,bone,scaling,trabeculae}
}

