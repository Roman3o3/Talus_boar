@article{pollathAstragalarMorphologyApproaching2019t,
  title = {Astragalar Morphology: {{Approaching}} the Cultural Trajectories of Wild and Domestic Sheep Applying {{Geometric Morphometrics}}},
  shorttitle = {Astragalar Morphology},
  author = {P?llath, Nadja and Schafberg, Renate and Peters, Joris},
  date = {2019-02-01},
  journaltitle = {Journal of Archaeological Science: Reports},
  shortjournal = {Journal of Archaeological Science: Reports},
  volume = {23},
  pages = {810--821},
  issn = {2352-409X},
  doi = {10.1016/j.jasrep.2018.12.004},
  url = {https://www.sciencedirect.com/science/article/pii/S2352409X18305182},
  urldate = {2024-01-29},
  abstract = {From the very beginning sheep keepers (un-)consciously selected animals with traits amenable to life and reproduction in an anthropogenic environment. Over the millennia sheep lineages developed which were adapted to the diverse landscapes and climates currently inhabited by the species. With time, human selection also fostered desired traits, such as docility, higher average milk production, abundant fleece or excessive fat depots, amongst others. However, verifying their appearance in the archaeological record is quite difficult applying standard archaeozoological methods. Here we present a Geometric Morphometric (GMM) approach that can shed light on these developments. Our study compares astragali of three wild and five domestic sheep populations. They come from prehistoric contexts in SW Asia dating to the Early Neolithic, Middle Chalcolithic and Late Bronze Age as well as modern reference series housed in natural history collections. We observe that ancient and modern wild sheep share the same astragalar morphology. Moreover, domestic sheep from Middle Chalcolithic Central Anatolia still resemble closely their Neolithic SE Anatolian wild relatives. Modern sheep populations from the United Kingdom and NW Germany form a distinct cluster, whereas sheep populations from Late Bronze Age Syria and particularly modern Karakul sheep clearly position away from all of the aforementioned groups, underscoring their distinct morphology. We conclude that the phenotypes preserved in the astragalar size and shape are at the same time a reflection of the respective population's geography, chronology and genetics.},
  keywords = {Archaeozoology,Astragalus,Geometric Morphometrics,Shape,Sheep morphotypes,Size},
  file = {C:\Users\Bordeaux Montaigne\Zotero\storage\MIZIIDFW\S2352409X18305182.html}
}

@article{pooleDenosumabRapidlyIncreases2015,
  title = {Denosumab {{Rapidly Increases Cortical Bone}} in {{Key Locations}} of the {{Femur}}: {{A 3D Bone Mapping Study}} in {{Women With Osteoporosis}}},
  shorttitle = {Denosumab {{Rapidly Increases Cortical Bone}} in {{Key Locations}} of the {{Femur}}},
  author = {Poole, Kenneth E.S. and Treece, Graham M and Gee, Andrew H and Brown, Jacques P and McClung, Michael R and Wang, Andrea and Libanati, Cesar},
  date = {2015-01-01},
  journaltitle = {Journal of Bone and Mineral Research},
  shortjournal = {Journal of Bone and Mineral Research},
  volume = {30},
