@thesis{davidModelisationVegetationHolocene2014d,
  type = {th?se de doctorat},
  title = {Mod?lisation de la v?g?tation holoc?ne du Nord-Ouest de la France : reconstruction de la chronologie et de l’?volution du couvert v?g?tal du Bassin parisien et du Massif armoricain},
  shorttitle = {Mod?lisation de la v?g?tation holoc?ne du Nord-Ouest de la France},
  author = {David, R?mi},
  date = {2014-04-04},
  institution = {Universit? de Rennes},
  location = {Rennes},
  url = {https://theses.hal.science/tel-01060260},
  urldate = {2024-04-10},
  abstract = {Comprendre la complexit? des relations entre nature et soci?t?s a toujours constitu? un d?fi pour les recherches dans le domaine de l'environnement. A l'?re de la compilation de donn?es dans de nombreuses disciplines, la mod?lisation constitue l'une des options les plus attractives pour comprendre et traduire les ?volutions du milieu naturel. Les objectifs principaux de ce travail de recherche sont 1) de restituer la structure du couvert v?g?tal r?gional et d'en retracer les ?volutions au cours de l'Holoc?ne au sein de deux r?gions du nord-ouest de la France, le Bassin parisien et le Massif armoricain, et 2) de les interpr?ter en termes de relations entre les soci?t?s pr?- et protohistoriques et leur milieu naturel. Pour cela, nous avons mis en place une d?marche m?thodologique visant ? 1) red?finir le cadre chronologique des s?quences polliniques rassembl?es pour les deux r?gions, 2) tester, adapter puis appliquer un mod?le de reconstitution quantitative du couvert v?g?tal, 3) d?finir une histoire de la v?g?tation estim?e relative ? chaque aire g?ographique mod?lis?e, 4) comparer ces histoires r?gionales et 5) les confronter aux informations disponibles en mati?re de densit?s de sites arch?ologiques et de proximit? de ces sites aux points de sondages polliniques. Des cadres structurant l'?volution du couvert v?g?tal pour chacune des deux r?gions ?tudi?es ressortent les grandes ?tapes de la mise en place des paysages au cours de l'Holoc?ne. Des sp?cificit?s r?gionales ont cependant pu ?tre identifi?es, tant en mati?re de chronologie de l'?volution de la v?g?tation estim?e qu'en mati?re de proportions relatives des taxons pr?sents. Une certaine singularit? doit donc ?tre reconnue ? l'?volution des paysages holoc?nes de chacune de ces deux parties du nord-ouest de la France. On remarque notamment des dynamiques et des niveaux d'anthropisation tout ? fait distincts entre Bassin parisien et Massif armoricain de la fin du M?solithique ? celle du N?olithique. A l'inverse, on retrouve de part et d'autre une certaine similarit? entre les proportions estim?es des taxons marqueurs d'anthropisation entre la fin du N?olithique et celle de l'?ge du Bronze. Des liens ?vidents entre les variations de l'impact anthropique enregistr? et celles de la densit? des occupations reconnues ont par ailleurs pu ?tre observ?s.},
  langid = {french},
  pagetotal = {285}
}

