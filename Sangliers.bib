@article{rowley-conwyWestwardHoSpread2011,
  title = {Westward {{Ho}}! {{The Spread}} of {{Agriculturalism}} from {{Central Europe}} to the {{Atlantic}}},
  author = {Rowley-Conwy, Peter},
  date = {2011-10},
  journaltitle = {Current Anthropology},
  volume = {52},
