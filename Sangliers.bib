  shorttitle = {Capture Effects in Wild Boar},
  author = {Brogi, Rudy and Brivio, Francesca and Bertolucci, Cristiano and Benazzi, Michele and Luccarini, Siriano and Cappai, Nadia and Bottero, Elisa and Pedrazzoli, Carlo and Columbano, Nicol? and Apollonio, Marco and Grignolio, Stefano},
