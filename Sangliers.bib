@article{cucchiEarlyNeolithicPig2011t,
  title = {Early {{Neolithic}} Pig Domestication at {{Jiahu}}, {{Henan Province}}, {{China}}: Clues from Molar Shape Analyses Using Geometric Morphometric Approaches},
  shorttitle = {Early {{Neolithic}} Pig Domestication at {{Jiahu}}, {{Henan Province}}, {{China}}},
  author = {Cucchi, T. and Hulme-Beaman, A. and Yuan, J. and Dobney, K.},
