@article{assifTrabecularBoneMorphology2024,
  title = {Trabecular Bone Morphology in Big Cats Reflects the Complex Diversity of Limb Use but Not Home Range Size or Daily Travel Distance},
  author = {Assif, Layne and Chirchir, Habiba},
  date = {2024},
  journaltitle = {The Anatomical Record},
  volume = {307},
  number = {1},
  pages = {208--222},
  issn = {1932-8494},
  doi = {10.1002/ar.25302},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ar.25302},
  urldate = {2024-04-26},
  abstract = {A relationship exists between mechanical loading and bone morphology. Although studies show a relationship between trabecular bone morphology and locomotor strategy in mammals, none of them have studied trabecular bone morphology in felid species occupying disparate and overlapping habitats. We investigate trabecular bone volume fraction (BVF) in the femoral and humeral heads, and distal tibia of four felid species (mountain lions, jaguars, cheetahs, and leopards) to identify whether there is a relationship between BVF and locomotor behavior. This study's goals are to identify whether felid species with high daily travel distance or large home range size have greater BVF compared with those with small daily travel distance or home range size, and whether BVF is correlated among the three elements of the fore and hindlimb studied. We quantified BVF in micro- and peripheral computed tomography images and found no significant differences across species in the femoral and humeral head (p {$>$} 0.05). However, in the distal tibia, results showed that leopards, mountain lions, and cheetahs have significantly greater (p {$<$} 0.05) BVF than jaguars. Despite differences in home range size and daily travel distance, the proximal elements did not reflect differences in BVF; however, the distal-most element did, suggesting decreased loading among jaguars. These findings suggest that the observed pattern of trabecular bone morphology is potentially due to the diversity in locomotor strategy of the forelimb. Additionally, these results imply that neither home range size nor daily travel distance are clear indicators of activity levels. A cautious approach is warranted in studying how loading influences trabecular morphology.},
  langid = {english},
  keywords = {bone density,bone volume fraction,felids,limb use,locomotor behavior,trabecular bone},
  file = {C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\NXRTZ37D\\Assif et Chirchir - 2024 - Trabecular bone morphology in big cats reflects th.pdf;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\2UHHJ375\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\64TENU29\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\9GBLJ339\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\I2EUGJCW\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\KBJIPULE\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\KVWCSA7G\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\LL3FEB34\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\SFT6HKX5\\ar.html;C\:\\Users\\Bordeaux Montaigne\\Zotero\\storage\\WLUFBF57\\ar.html}
}

