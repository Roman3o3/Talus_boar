  series = {Collection {{Sciences}} Arch?ologiques},
  pages = {197--216},
  publisher = {?ditions des archives contemporaines},
  location = {Paris},
  abstract = {Un poisson nage, un oiseau vole, un ongul? galope, un humain marche… gr?ce ? un ensemble de muscles et de fonctions pr?cises et compl?mentaires qui prennent appui sur une structure rigide, le squelette, dont l'organisation est partag?e par tous ces ?tres vivants, vert?br?s. On peut rajouter aux os de ce squelette des ?l?ments essentiels pour l'alimentation ou la d?fense : les dents. Os et dents r?sistent au passage du temps et ? la d?composition du corps qui suit la mort des organismes. Ils peuvent se conserver tr?s longtemps lorsque les conditions sont favorables. Ils deviennent alors des fossiles, t?moins pr?cieux de la vie du pass?. Les restes squelettiques constituent des archives biologiques et environnementales, archives culturelles lorsqu'ils sont retrouv?s dans un contexte arch?ologique, qui plus est lorsqu'ils ont ?t? fa?onn?s, transform?s par l'homme. Ils sont porteurs d'autant de « messages » qui peuvent ?tre d?crypt?s par des approches arch?om?triques. L'arch?om?trie ?tudie, par des mesures instrument?es, des caract?res le plus souvent inaccessibles ? l'observation visuelle. Elle met en oeuvre des connaissances et des m?thodes relevant des sciences de la Terre et de la Vie, des sciences environnementales et de la physico-chimie, ? des ?chelles d'observation allant de la macro-? la nanostructure. Toutes ces approches, sans cesse enrichies par les d?veloppements techniques et m?thodologiques, ouvrent des perspectives largement renouvel?es depuis une quinzaine d'ann?es. Nous en pr?sentons ici un instantan?, avec pour objectif principal d'offrir un panorama de la diversit? de ces approches, dans une d?marche didactique pr?sentant aussi bien les techniques et les m?thodes, que les applications. Messages d'os est un manuel destin? ? un large public curieux des enseignements que le squelette apporte ? nos connaissances des communaut?s animales et soci?t?s humaines du pass?. Os et dents, signes des temps pass?s… Image en microscopie confocale de fl uorescence du r?seau lacuno-canaliculaire d'os bovin.},
  bookauthor = {Balasse, Marie and Brugal, Jean-Philip and Dauphin, Yannicke and Geigl, Eva-Maria and Oberlin, Christine and Reiche, Ina},
  isbn = {978-2-8130-0164-1}
}

@article{cucchiNewInsightsPig2009d,
  title = {New Insights into Pig Taxonomy, Domestication and Human Dispersal in {{Island South East Asia}}: Molar Shape Analysis of {{Sus}} Remains from {{Niah Caves}}, {{Sarawak}}},
  shorttitle = {New Insights into Pig Taxonomy, Domestication and Human Dispersal in {{Island South East Asia}}},
  author = {Cucchi, T. and Fujita, M. and Dobney, K.},
  date = {2009},
  journaltitle = {International Journal of Osteoarchaeology},
  volume = {19},
  number = {4},
  pages = {508--530},
  issn = {1099-1212},
  doi = {10.1002/oa.974},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/oa.974},
  urldate = {2024-03-20},
  abstract = {Despite the almost ubiquitous presence of Holocene Sus remains in Island South East Asia (ISEA), the domestication of pigs and their dispersal in this region remains somewhat confused. Using molar shape geometric morphometric analysis, archaeological Sus dental series from the Sarawak caves of Niah (Aceramic, Neolithic) and Lobang Kudih (Ming period), are compared with extant ISEA wild pigs to establish their taxonomic status and to further explore the evidence for local domestication or introduction of allochthonous populations. Results on modern data-sets show that outline analysis of the third lower molar achieves highly significant inter- and intra-specific differentiation among ISEA wild pigs, with a phenotypic divergence structure displaying congruence with molecular phylogenies. Both tooth shape and mtDNA evidence strongly suggest the so-called wild pigs of New Guinea to be the descendants of pigs domesticated in mainland South East Asia and introduced by early farmers to ISEA. None of our data provide clear evidence for a Neolithic introduction of domesticated pigs, or for the local domestication of indigenous bearded pigs, since all Aceramic and Neolithic pigs from Niah cave have been identified as indigenous bearded pig (Sus barbatus). Local domestication experiments by foragers have not been ruled out, since evidence of hypoplasia as a marker of domestication processes has been observed. Introduction of allochthonous domestic pigs between the Late Neolithic and the Metal Age is suggested, since this small domestic breed from Lobang Kudih cave displays strong phenetic relationships with S. scrofa of New Guinea, recognised by molecular phylogeography as part of the ‘Pacific Clade’ linked both with Lapita and Polynesian dispersals. Introduction of celebensis-like species has also been suggested. Future studies should incorporate broader time-scale and geographical framework data-sets to strengthen these assumptions. Copyright © 2008 John Wiley \& Sons, Ltd.},
  issue = {4},
  langid = {english},
  keywords = {Elliptic Fourier transform,geometric morphometrics,Island South East Asia,Neolithic,teeth,zooarchaeology},
  file = {C:\Users\Bordeaux Montaigne\Zotero\storage\6AZTWTPR\oa.html}
}

@article{cucchiSocialComplexificationPig2016d,
  title = {Social {{Complexification}} and {{Pig}} ({{Sus}} Scrofa) {{Husbandry}} in {{Ancient China}}: {{A Combined Geometric Morphometric}} and {{Isotopic Approach}}},
  shorttitle = {Social {{Complexification}} and {{Pig}} ({{Sus}} Scrofa) {{Husbandry}} in {{Ancient China}}},
  author = {Cucchi, Thomas and Dai, Lingling and Balasse, Marie and Zhao, Chunqing and Gao, Jiangtao and Hu, Yaowu and Yuan, Jing and Vigne, Jean-Denis},
  date = {2016-08},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
  volume = {11},
  number = {7},
  pages = {e0162134},
  publisher = {Public Library of Science},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0158523},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0158523},
  urldate = {2024-03-20},
  abstract = {Pigs have played a major role in the economic, social and symbolic systems of China since the Early Neolithic more than 8,000 years ago. However, the interaction between the history of pig domestication and transformations in Chinese society since then, have not been fully explored. In this paper, we investigated the co-evolution from the earliest farming communities through to the new political and economic models of state-like societies, up to the Chinese Empire, using 5,000 years of archaeological records from the Xiawanggang (XWG) and Xinzhai (XZ) sites (Henan Province). To trace the changes of pig populations against husbandry practices, we combined the geometric morphometric analysis of dental traits with a study of the stable carbon and nitrogen isotope ratios from bone collagen. The domestication process intensified during the Neolithic Yangshao, prompted by greater selective pressure and/or better herd control against wild introgression. After that, pig farming, in XWG, relied on local livestock and a gradual change of husbandry practices overtime. This was characterized by a gentle increase in millet foddering and animal protein intake, until a complete change over to household management during the Han dynasty. The only rupture in this steady trend of husbandry occurred during the Longshan period, with the appearance of small sized and idiosyncratic pigs with specific feeding practices (relying on millet and household scraps). From three exploratory hypothesis, we explored the possibility of anti-elite pig production in XWG during the Longshan period, as a means to resist incorporation into a new economic model promoting intensified domestic production. This exploratory hypothesis is the most suitable to our dataset; however, numerous areas need to be explored further in order to adequately document the role of pigs in the rise of China’s complex societies.},
  issue = {8},
  langid = {english},
  keywords = {Animal husbandry,Archaeology,Diet,Domestic animals,Millet,Neolithic period,Swine,Trophic interactions}
}

