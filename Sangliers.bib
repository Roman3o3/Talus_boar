@article{rosenbergHallanCemiPig1998d,
  title = {Hallan {{?emi}}, Pig Husbandry, and Post-Pleistocene Adaptations along the {{Taurus-Zagros Arc}} ({{Turkey}})},
  author = {Rosenberg, Michael and Nesbitt, R. Mark and Redding, Richard and Peasnall, Brian L.},
  date = {1998},
  journaltitle = {Pal?orient},
  volume = {24},
  number = {1},
  eprint = {41492752},
  eprinttype = {jstor},
  pages = {25--41},
  publisher = {{[Paleorient and CNRS Editions, CNRS Editions]}},
  issn = {0153-9345},
  url = {https://www.jstor.org/stable/41492752},
  urldate = {2024-04-02},
  abstract = {Recent work at Hallan ?emi and other round house horizon sites in eastern Anatolia indicates that the Taurus-Zagros flanks were a second autochthonous center of neolithization in southwestern Asia. Fully settled complex hunter-gatherer societies are in existence in this area by the late Younger Dryas. These settled village societies were based on adaptations that did not involve cereal exploitation, presumably because cereals were absent in this area during the late Younger Dryas. Instead, these adaptations revolved around the exploitation of nuts and pulses, plus the hunting of ovicaprids and deer supplemented by early experiments with animal husbandry involving pigs. They are thus distinct from those that served as the foundation for the earliest sedentary societies in the Levant. Most current attempts to explain the beginnings of settled village life in southwestern Asia are based solely on Levantine data, which until recently were virtually all that were available. The Anatolian data do not conform to the Levantine pattern and thus raise serious questions about the general validity of these models. Les travaux r?cents conduits ? Hallan ?emi et sur d'autres sites qui se placent en Anatolie orientale sur l'horizon ? maisons circulaires indiquent que les flancs du Taurus-Zagros furent un deuxi?me centre autochtone de n?olithisation en Asie du sud-ouest. Des soci?t?s tout ? fait s?dentaris?es de chasseurs-cueilleurs ?taient pr?sentes dans cette r?gion au Dryas r?cent. Ces groupes ?tablis en village avaient une subsistance qui ne reposait pas sur l'exploitation des c?r?ales absentes dans cette r?gion au Dryas r?cent, mais ils s'adapt?rent ? l'exploitation de noix et de l?gumineuses ? laquelle s'ajoutaient la chasse d'ovicaprid?s et de cerfs, ainsi que les premiers essais d'?levage, y compris celui de cochons. Ainsi, ces soci?t?s se distinguent-elles de celles qui constituaient les premi?res soci?t?s s?dentaires au Levant. Jusqu'? pr?sent, les tentatives d'explication du d?but de la vie villageoise reposaient uniquement sur des donn?es provenant du Levant, seules disponibles. Les donn?es en provenance d'Anatolie ne se conforment pas ? celles du Levant et soul?vent ainsi des questions s?rieuses sur la validit? de la g?n?ralisation de ces mod?les.},
  issue = {1}
}

