@article{krause-kyoraUseDomesticatedPigs2013,
  title = {Use of Domesticated Pigs by {{Mesolithic}} Hunter-Gatherers in Northwestern {{Europe}}},
  author = {Krause-Kyora, Ben and Makarewicz, Cheryl and Evin, Allowen and Flink, Linus Girdland and Dobney, Keith and Larson, Greger and Hartz, S?nke and Schreiber, Stefan and family=Carnap-Bornheim, given=Claus, prefix=von, useprefix=true and family=Wurmb-Schwark, given=Nicole, prefix=von, useprefix=true and Nebel, Almut},
