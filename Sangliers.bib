  keywords = {Domestication,Geometric morphometrics,Morphology,Sliding semilandmarks,Teeth,Zooarchaeology},
  file = {C:\Users\Bordeaux Montaigne\Zotero\storage\FWW6Y59N\S0305440310002645.html}
}

